<LinkTo @route="cart" class="d-flex justify-content-end p-2 cart-icon">
  <i class="fa fa-5x fa-shopping-cart icon"></i>
  {{!-- this.shoppingCartService.cartList.length --}}
  {{#if cartcount}}
    
  <span class="countIcon">{{ cartcount}}</span>
  {{/if}}

</LinkTo>

<main class="ml-3">
  {{yield}}
</main>